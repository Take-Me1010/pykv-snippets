{
    "publisher": "Take-Me1010",
    "name": "pykv-snippets",
    "displayName": "Python-Kivy-snippets",
    "description": "code snippets for python Kivy",
    "repository": {
        "type": "git",
        "url": "https://github.com/Take-Me1010/pykv-snippets"
    },
    "icon": "image/icon.png",
    "license": "SEE LICENSE(MIT)",
    "version": "1.0.10",
    "engines": {
        "vscode": "^1.47.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "Python",
        "Kivy",
        "kv",
        "snippet"
    ],
    "contributes": {
        "languages": [
            {
                "id": "kv",
                "aliases": [
                    "KvLang",
                    "kv"
                ],
                "extensions": [
                    ".kv"
                ]
            },
            {
                "id": "python",
                "aliases": [
                    "python",
                    "py"
                ],
                "extensions": [
                    ".py"
                ]
            }
        ],
        "snippets": [
            {
                "language": "kv",
                "path": "./snippets/kivy.json"
            },
            {
                "language": "python",
                "path": "./snippets/python.json"
            },
            {
                "language": "python",
                "path": "./snippets/keywords.json"
            }
        ]
    }
}
